{
	"name": "myapp",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"start": "NODE_ENV=production node ./bin/www",
		"dev": "NODE_ENV=development DEBUG=myapp:* node --watch ./bin/www",
		"build": "biome check --write && echo 'Build checks passed!'",
		"test": "jest",
		"format": "biome format --write .",
		"format:check": "biome format .",
		"lint": "biome lint --write .",
		"lint:check": "biome lint .",
		"check": "biome check --write .",
		"check:ci": "biome check .",
		"clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install"
	},
	"dependencies": {
		"ajax": "0.0.4",
		"axios": "^1.13.2",
		"cheerio": "^1.1.2",
		"cookie-parser": "~1.4.7",
		"debug": "~4.4.3",
		"dotenv": "^17.2.3",
		"express": "~5.2.1",
		"http-errors": "~2.0.1",
		"morgan": "~1.10.1",
		"pug": "^3.0.3"
	}
}
